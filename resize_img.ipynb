    "import cv2\n",
    "\n",
    "path = \"static/images/FineGym_l.png\"\n",
    "img = cv2.imread(path, cv2.IMREAD_COLOR)\n",
    "\n",
    "img = cv2.resize(img, (0, 0), fx=0.125, fy=0.125)\n",
    "# cut the top and bottom\n",
    "# img = img[30:img.shape[0]-30, :]\n",
    "\n",
    "# save the image\n",
    "cv2.imwrite(\"static/images/FineGym_l.png\", img)\n"
   ]
